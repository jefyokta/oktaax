{
    "name": "jefyokta/oktaax",
    "description": "Oktaax a openswoole http server library",
    "type": "library",
    "keywords":["http-library","openswoole","php-server","openswoole-library","express.php","express-php","php-microframework","event-driven","php-websocket","websocket","php","real-time"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oktaax\\": "src/",
            "Oktaax\\Http\\Middleware\\":"src/Http/Middlewares/",
            "Appx\\":"app/"
        },
        "files":[
            "src/Utils/Functions.php"
        ]
    },
    "authors": [
        {
            "name": "Jepi Oktamipa",
            "email": "jefyokta50@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-openswoole": "*",
        "illuminate/view": "^11.23",
        "illuminate/container": "^11.23",
        "firebase/php-jwt": "^6.10"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jefyokta/oktaax"
        }
    ],
    "require-dev": {
        "openswoole/ide-helper": "~22.0.1"
    }
}
